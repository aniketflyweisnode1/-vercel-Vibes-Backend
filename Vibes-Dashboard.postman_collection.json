        {
          "name": "Create Client",
          "request": {
            "method": "POST",
            "header": [
              { "key": "Content-Type", "value": "application/json" }
            ],
            "url": {
              "raw": "{{vibes}}/api/vendor/corporate-dashboard-client/create",
              "host": ["{{vibes}}"],
              "path": ["api", "vendor", "corporate-dashboard-client", "create"]
            },
            "body": {
              "mode": "raw",
              "raw": "{\n  \"CompanyName\": \"Acme Corp\",\n  \"industry\": \"Technology\",\n  \"EmployeeCount\": 500,\n  \"ContactEmail\": \"contact@acme.com\",\n  \"Plan_id\": 1,\n  \"Status\": true\n}"
            }
